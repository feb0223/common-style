// mixins
percentage($n)
	if $n == 0
		0
	else
		unit($n * 100, "%")
@import bootstrapPath + 'mixins/clearfix.styl'
@import bootstrapPath + 'mixins/grid-framework.styl'
@import bootstrapPath + 'mixins/grid.styl'
@import bootstrapPath + 'mixins/responsive-visibility.styl'

// overwrite variables
$grid-gutter-width = 0
$container-sm = inherit
$container-md = inherit
$container-lg = inherit
$screen-xs = 320px
$screen-sm = $sp-width + 1
if $tablet-width != 0
	$screen-md = $tablet-width + 1

@import bootstrapPath + 'variables.styl'
@import bootstrapPath + 'utilities.styl'

// components
@import bootstrapPath + 'grid.styl'
@import bootstrapPath + 'responsive-utilities.styl'

// scaffolding
*
	box-sizing border-box
*:before,
*:after
	box-sizing border-box
.col-xs-1, .col-sm-1, .col-md-1, .col-lg-1, .col-xs-2, .col-sm-2, .col-md-2, .col-lg-2, .col-xs-3, .col-sm-3, .col-md-3, .col-lg-3, .col-xs-4, .col-sm-4, .col-md-4, .col-lg-4, .col-xs-5, .col-sm-5, .col-md-5, .col-lg-5, .col-xs-6, .col-sm-6, .col-md-6, .col-lg-6, .col-xs-7, .col-sm-7, .col-md-7, .col-lg-7, .col-xs-8, .col-sm-8, .col-md-8, .col-lg-8, .col-xs-9, .col-sm-9, .col-md-9, .col-lg-9, .col-xs-10, .col-sm-10, .col-md-10, .col-lg-10, .col-xs-11, .col-sm-11, .col-md-11, .col-lg-11, .col-xs-12, .col-sm-12, .col-md-12, .col-lg-12
	position static